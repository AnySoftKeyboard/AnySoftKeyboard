<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- exclude the generated code -->
    <Match>
        <Class name="~.*\.R(\$.*)?" />
    </Match>

    <!-- Checkstyle is verifying case default -->
    <Match>
        <Bug pattern="SF_SWITCH_NO_DEFAULT"/>
    </Match>
</FindBugsFilter>
