<words>
    <!-- this dictionary is used for common typing mistakes -->
    <!-- the attribute 'src' will be replace by the node's value: -->
    <!-- drot will be converted to droit -->
    <word src="drot">droit</word>   <!-- press on 'i' failed -->
    <word src="dtoit">droit</word>  <!-- press on 'r' become 't' -->
    <word src="cest">c'est</word>   <!-- miss ' -->
    <word src="sest">s'est</word>
    <word src="jai">j'ai</word>
</words>
