<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ask="http://schemas.android.com/apk/res-auto"
    android:keyWidth="10%p"
    android:keyHeight="@integer/key_normal_height"
    ask:autoCap="false">

    <!-- Top row -->
    <Row android:rowEdgeFlags="top">
        <Key
            android:codes="@integer/key_code_esc"
            android:keyEdgeFlags="left"
            android:keyLabel="Esc"
            ask:isFunctional="true" />
        <Key android:codes="@integer/key_code_tab" />
        <Key
            android:codes="@integer/key_code_ctrl"
            android:isModifier="true"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_arrow_left"
            android:horizontalGap="5%p"
            android:isRepeatable="true"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_arrow_up"
            android:isRepeatable="true"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_arrow_down"
            android:isRepeatable="true"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_arrow_right"
            android:isRepeatable="true"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_move_end"
            android:horizontalGap="5%p"
            android:keyLabel="Hom"
            ask:isFunctional="true" />
        <Key
            android:codes="@integer/key_code_move_home"
            android:keyEdgeFlags="right"
            android:keyLabel="End"
            ask:isFunctional="true" />
    </Row>

    <Row>
        <!-- Q -->
        <Key
            android:codes="\u0642"
            android:keyEdgeFlags="left"
            android:popupCharacters="\u0661\1\u0652"
            ask:hintLabel="\u0661"
            ask:shiftedCodes="\u0652" />
        <!-- W -->
        <Key
            android:codes="\u0648"
            android:popupCharacters="\u0662\2\u0624"
            ask:hintLabel="\u0662"
            ask:shiftedCodes="\u0624" />
        <!-- E -->
        <Key
            android:codes="\u0639"
            android:popupCharacters="\u0663\3\u0611"
            ask:hintLabel="\u0663"
            ask:shiftedCodes="\u0611" />
        <!-- R -->
        <Key
            android:codes="\u0631"
            android:popupKeyboard="@xml/urdu_with_symbols_r"
            ask:hintLabel="\u06F4"
            ask:shiftedCodes="\u0691" />
        <!-- T -->
        <Key
            android:codes="\u062A"
            android:popupKeyboard="@xml/urdu_with_symbols_t"
            ask:hintLabel="\u06F5"
            ask:shiftedCodes="\u0679" />
        <!-- Y -->
        <Key
            android:codes="\u06D2"
            android:popupKeyboard="@xml/urdu_with_symbols_y"
            ask:hintLabel="\u0666"
            ask:shiftedCodes="\u064E" />
        <!-- U -->
        <Key
            android:codes="\u0621"
            android:popupCharacters="\u0667\7\u0654\u0655"
            ask:hintLabel="\u0667"
            ask:shiftedCodes="\u0654" />
        <!-- I -->
        <Key
            android:codes="\u06CC"
            android:popupKeyboard="@xml/urdu_with_symbols_i"
            ask:hintLabel="\u0668"
            ask:shiftedCodes="\u0650" />
        <!-- O -->
        <Key
            android:codes="\u06C1"
            android:popupCharacters="\u0669\9\u06BE\u0647"
            ask:hintLabel="\u0669"
            ask:shiftedCodes="\u06BE" />
        <!-- P -->
        <Key
            android:codes="\u067E"
            android:keyEdgeFlags="right"
            android:popupCharacters="\u0660\0\u064F\u064C\u0657"
            ask:hintLabel="\u0660"
            ask:shiftedCodes="\u064F" />
    </Row>

    <Row>
        <!-- A -->
        <Key
            android:codes="\u0627"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:popupCharacters="\@\u0622\u0670\u0656\u0653"
            ask:hintLabel="\@"
            ask:shiftedCodes="\u0622" />
        <!-- S -->
        <Key
            android:codes="\u0633"
            android:popupCharacters="$\u0635\u0601\u0603\u0610"
            ask:hintLabel="$"
            ask:shiftedCodes="\u0635" />
        <!-- D -->
        <Key
            android:codes="\u062F"
            android:popupCharacters="#\u0688"
            ask:hintLabel="#"
            ask:shiftedCodes="\u0688" />
        <!-- F -->
        <Key
            android:codes="\u0641"
            android:popupCharacters="\u066A%^"
            ask:hintLabel="\u066A"
            ask:shiftedCodes="\uFDFA" />
        <!-- G -->
        <Key
            android:codes="\u06AF"
            android:popupCharacters="&amp;\u063A"
            ask:hintLabel="&amp;"
            ask:shiftedCodes="\u063A" />
        <!-- H -->
        <Key
            android:codes="\u062D"
            android:popupCharacters="`~"
            ask:hintLabel="`"
            ask:shiftedCodes="\uFDFB" />
        <!-- J -->
        <Key
            android:codes="\u062C"
            android:popupKeyboard="@xml/urdu_with_symbols_j"
            ask:hintLabel="("
            ask:shiftedCodes="\u0636" />
        <!-- K -->
        <Key
            android:codes="\u06A9"
            android:popupKeyboard="@xml/urdu_with_symbols_k"
            ask:hintLabel=")"
            ask:shiftedCodes="\u062E" />
        <!-- L -->
        <Key
            android:codes="\u0644"
            android:keyEdgeFlags="right"
            android:popupCharacters="_"
            ask:hintLabel="_"
            ask:shiftedCodes="\uFDF2" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/key_code_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="15%p"
            android:keyEdgeFlags="left" />
        <!-- Z -->
        <Key
            android:codes="\u0630"
            android:popupCharacters="/÷\u0632"
            ask:hintLabel="/"
            ask:shiftedCodes="\u0632" />
        <!-- X -->
        <Key
            android:codes="\u0634"
            android:popupKeyboard="@xml/urdu_with_symbols_x"
            ask:hintLabel="*"
            ask:shiftedCodes="\u0698" />
        <!-- C -->
        <Key
            android:codes="\u0686"
            android:popupKeyboard="@xml/urdu_with_symbols_c"
            ask:hintLabel="-"
            ask:shiftedCodes="\u062B" />
        <!-- V -->
        <Key
            android:codes="\u0637"
            android:popupKeyboard="@xml/urdu_with_symbols_v"
            ask:hintLabel="+"
            ask:shiftedCodes="\u0638" />
        <!-- B -->
        <Key
            android:codes="\u0628"
            android:popupCharacters="\\|"
            ask:hintLabel="\\"
            ask:isShiftAlways="true"
            ask:shiftedCodes="\uFDFD" />
        <!-- N -->
        <Key
            android:codes="\u0646"
            android:popupCharacters="\u061B:\u06BA"
            ask:hintLabel="\u061B"
            ask:shiftedCodes="\u06BA" />
        <!-- M -->
        <Key
            android:codes="\u0645"
            android:popupCharacters="\u061F!"
            ask:hintLabel="\u061F"
            ask:shiftedCodes="\uFDF4" />
        <Key
            android:codes="@integer/key_code_delete"
            android:isRepeatable="true"
            android:keyWidth="15%p"
            android:keyEdgeFlags="right" />
    </Row>

    <!-- Bottom row -->
    <Row android:rowEdgeFlags="bottom">

        <Key
            android:codes="@integer/key_code_keyboard_mode_change"
            android:keyWidth="12.5%p"
            android:keyEdgeFlags="left"
            ask:isFunctional="true"
            ask:keyDynamicEmblem="icon"
            ask:longPressCode="@integer/key_code_mode_alphabet_popup" />

        <!-- comma -->
        <Key
            android:codes="\u060C"
            ask:isFunctional="true" />

        <Key
            android:codes="@integer/key_code_quick_text_popup"
            ask:isFunctional="true"
            ask:longPressCode="@integer/key_code_quick_text" />

        <Key
            android:codes="@integer/key_code_space"
            android:keyWidth="35%p"
            ask:isFunctional="true" />

        <Key
            android:codes="&quot;"
            android:popupCharacters="\'"
            ask:hintLabel=" "
            ask:isFunctional="true" />

        <!-- full stop -->
        <Key
            android:codes="\u06D4"
            android:popupKeyboard="@xml/urdu_with_symbols_full_stop"
            ask:hintLabel=" "
            ask:isFunctional="true" />

        <Key
            android:codes="@integer/key_code_enter"
            android:keyWidth="12.5%p"
            android:keyEdgeFlags="right"
            ask:isFunctional="true"
            ask:longPressCode="@integer/key_code_settings" />
    </Row>
</Keyboard>

